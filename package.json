{
  "name": "base",
  "version": "0.1.0",
  "scripts": {
    "start": "./clear.sh && gulp copy-jit && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "only-run": "gulp copy-jit && concurrently \"npm run tsc\" ",
    "lite": "lite-server",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "aot": "ngc -p tsconfig-aot.json",
    "rollup": "rollup -c rollup-config.js",
    "start-aot": "npm run aot && npm run rollup && gulp copy-aot && npm run lite",
    "start-gzip": "npm run aot && npm run rollup && gulp copy-aot-gzip && http-server"
  },
